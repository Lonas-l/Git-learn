[core]
	repositoryformatversion = 0
	filemode = true
	bare = false
	logallrefupdates = true

[alias]
 	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
	get = git
	got = git
	
[remote "origin"]
	url = https://github.com/Lonas-l/Git-learn.git
	fetch = +refs/heads/*:refs/remotes/origin/*
[branch "master"]
	remote = origin
	merge = refs/heads/master
